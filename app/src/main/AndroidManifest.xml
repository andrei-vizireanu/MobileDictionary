<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andrei.dictionar"
    android:versionCode="1"
    android:versionName="Beta">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SET_ALARM"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dictionary"
        android:label="Mobile Dictionary"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlegereaDictionarului" />
        <activity android:name=".Ro_Eng" />
        <activity android:name=".AddWord" />
        <activity android:name=".SearchWord" />
        <activity android:name=".List_Detail" />
        <activity android:name=".DeleteWord" />
        <activity android:name=".AdapterItems" />
        <activity android:name=".RandomWord" />
        <activity android:name=".Repeaiting_activity"/>
        <receiver android:name=".Notification_reciever"/>
        <activity android:name=".Edit_Word"></activity>
    </application>

</manifest>